1. head -n10 users.txt
2. tail -n5 users.txt
3. tail -n+11 users.txt
4. tail -n+11 users.txt | head -n10
5. cut -b 1 users.txt
6. cut -b 1,3,5 users.txt
7. cat users.txt | grep -v ^[#] | cut -d: -f1
8. cat users.txt | grep -v ^[#,_] | cut -d: -f1
7. cat users.txt | grep -v ^[#] | cut -d: -f3
8. cat -n users.txt | sort -rn
9. cat users.txt | sort -t: -k3 -n
7. cat users.txt | grep -v ^[#] | cut -d: -f4 | sort -n